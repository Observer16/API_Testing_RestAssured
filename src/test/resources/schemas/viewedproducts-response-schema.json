{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "viewedProducts": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": { "type": "number" },
              "title": { "type": "string" },
              "article": { "type": "string" },
              "rating": { "type": "number" },
              "images": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": { "type": "string" },
                    "title": { "type": "string" },
                    "path": { "type": "string" },
                    "alternative": { "type": "string" }
                  }
                }
              },
              "price": {
                "type": "object",
                "properties": {
                  "main": {
                    "type": "object",
                    "properties": {
                      "price": { "type": "number" },
                      "currency": { "type": "string" },
                      "unit": { "type": "string" },
                      "basePrice": { "type": "number" }
                    }
                  },
                  "additional": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "price": { "type": "number" },
                        "currency": { "type": "string" },
                        "unit": { "type": "string" }
                      }
                    }
                  },
                  "accounting": {
                    "type": "object",
                    "properties": {
                      "price": { "type": "number" },
                      "currency": { "type": "string" },
                      "unit": { "type": "string" },
                      "basePrice": { "type": "number" }
                    }
                  },
                  "bonus": { "type": "number" },
                  "extraBonus": { "type": "number" }
                }
              },
              "availability": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "title": { "type": "string" },
                    "type": { "type": "string" },
                    "availabilityCount": {
                      "type": "object",
                      "properties": {
                        "amount": { "type": "number" },
                        "unit": { "type": "string" },
                        "text": { "type": "string" }
                      }
                    },
                    "prompt": {
                      "type": "object",
                      "properties": {
                        "title": { "type": "string" },
                        "text": { "type": "string" }
                      }
                    },
                    "availabilityDate": {
                      "type": "object",
                      "properties": {
                        "title": { "type": "string" },
                        "planDate": { "type": "string" },
                        "planDateText": { "type": "string" },
                        "prompt": {
                          "type": "object",
                          "properties": {
                            "title": { "type": "string" },
                            "text": { "type": "string" }
                          }
                        }
                      }
                    },
                    "pickups": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": { "type": "number" },
                          "title": { "type": "string" },
                          "date": { "type": "string" },
                          "dateText": { "type": "string" },
                          "address": { "type": "string" },
                          "latitude": { "type": "string" },
                          "longitude": { "type": "string" },
                          "workingHours": {
                            "type": "object",
                            "properties": {
                              "title": { "type": "string" },
                              "from": { "type": "string" },
                              "to": { "type": "string" }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              },
              "availableType": { "type": "string" },
              "sectionId": { "type": "number" },
              "inBasket": { "type": "number" },
              "quantityInWishLists": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "wishListId": { "type": "number" },
                    "quantity": { "type": "number" }
                  }
                }
              },
              "multiple": { "type": "number" },
              "is24": { "type": "boolean" },
              "isNoticeReceiptExist": { "type": "boolean" },
              "nameplates": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "type": { "type": "string" },
                    "title": { "type": "string" },
                    "id": { "type": "number" },
                    "url": { "type": "string" },
                    "bgColor": { "type": "string" },
                    "textColor": { "type": "string" }
                  }
                }
              },
              "code": { "type": "string" }
            },
            "required": ["id", "title"]
          }
        }
      },
      "required": ["viewedProducts"]
    }
  },
  "required": ["data"]
}
